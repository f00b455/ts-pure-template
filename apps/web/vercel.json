{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "buildCommand": "cd ../.. && pnpm turbo build --filter=@ts-template/web",
  "installCommand": "cd ../.. && pnpm install",
  "devCommand": "pnpm dev",
  "env": {
    "NEXT_PUBLIC_API_URL": "@next_public_api_url"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}