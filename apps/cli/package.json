{
  "name": "@ts-template/cli",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --format esm",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:cucumber": "cucumber-js",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0"
  },
  "bin": {
    "hello-cli": "./dist/index.js"
  },
  "dependencies": {
    "@ts-template/lib-foo": "workspace:*",
    "boxen": "^8.0.1",
    "cac": "^6.7.14",
    "cli-progress": "^3.12.0",
    "gradient-string": "^3.0.0",
    "nanospinner": "^1.2.0",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^11.2.1",
    "@types/cli-progress": "^3.11.6",
    "@types/gradient-string": "^1.1.6",
    "@types/node": "^22.10.5",
    "eslint": "^9.18.0",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}