    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@ts-template/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.2",
    "vitest": "^1.4.0"
  }
}